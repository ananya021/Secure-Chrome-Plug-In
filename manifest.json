{
    "manifest_version": 3,
    "name": "SafeWeb",
    "version": "1.0",
    "description": "Detects and blocks phishing attempts and malware downloads.",
    "permissions": ["webRequest", "webNavigation", "downloads", "storage"],
    "background": {
      "service_worker": "js/background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.jpg",
        "48": "images/icon48.jpg",
        "128": "images/icon128.jpg"
      }
    },
    "icons": {
      "16": "images/icon16.jpg",
      "48": "images/icon48.jpg",
      "128": "images/icon128.jpg"
    }
  }
  
